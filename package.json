{
    "name": "matter-yaml",
    "version": "2.0.2",
    "homepage": "https://alexstevovich.com/a/matter-yaml-nodejs",
    "author": {
        "name": "Alex Stevovich",
        "email": "dev@alexstevovich.com",
        "url": "https://alexstevovich.com"
    },
    "description": "Parses and serializes YAML front matter in text documents.",
    "type": "module",
    "main": "./src/index.js",
    "files": [
        "./src"
    ],
    "license": "Apache-2.0",
    "scripts": {
