    },
    "devDependencies": {
        "eslint": "^9.36.0",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^5.5.4",
        "poshify": "^1.0.0",
        "prettier": "^3.6.2",
        "vitest": "^3.2.4"
    }
}
